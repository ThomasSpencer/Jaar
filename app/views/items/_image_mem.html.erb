<%= form_for [@jar, @item] do |f| %>
<%= f.hidden_field :type_id, {value: @type_id} %>
  <div class="field">
    <%= f.label :type_data, 'Upload Your Image Here' %>
    <%= f.file_field :type_data, id: 'fileupload' %>
  </div>
  <div class="comment-field">
    <%= f.label :comment %>
    <%= f.text_field :comment %>
  </div>
<% end %>
