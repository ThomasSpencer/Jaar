<%= form_for [@jar, @item] do |f| %>
<%= f.hidden_field :type_id, {value: @type_id} %>
  <div class="comment-field">
    <%= f.label :comment, "Please Provide Youtube Link"%>
    <%= f.text_field :comment %>
  </div>
  <div class="action">
    <%= f.submit "Submit Link", id: 'submit-new-mem' %>
  </div>
<% end %>
